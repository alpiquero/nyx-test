            {
              "qualifier": "{{#short5}}{{releaseScope.finalCommit}}{{/short5}}",
              "position": "BUILD"
            },
            {
              "qualifier": "{{#timestampYYYYMMDDHHMMSS}}{{timestamp}}{{/timestampYYYYMMDDHHMMSS}}",
